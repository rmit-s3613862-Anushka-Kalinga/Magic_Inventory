<script src="/js/angular.min.js"></script>

<div ng-app="myApp" ng-controller="orderHistoryCtrl">
    <h2>Order History</h2>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>OrderID</th>
                <th>userName</th>
                <th>productID</th>
                <th>storeID</th>
                <th>quantity</th>
                <th>price</th>
                <th>orderDate</th>
                <th>orderNumber</th>

</tr>
        <thead>
        <tbody>
            <tr ng-repeat="x in orderHistory">
                <td>{{ x.orderID }}</td>
                <td>{{ x.userName }}</td>
                <td>{{ x.productID }}</td>
                <td>{{ x.storeID }}</td>
                <td>{{ x.quantity }}</td>
                <td>{{ x.price | currency }}</td>
                <td>{{ x.orderDate }}</td>
                <td>{{ x.orderNumber}}</td>
            </tr>
        </tbody>
    </table>
</div>

<script>
    var app = angular.module("myApp", []);
    app.controller("orderHistoryCtrl", function($scope, $http) {
        $http.get("http://localhost:60186//api/Orderhistories").then(function (response) { $scope.orderHistory = response.data; });
    });
</script>